def prime():
    l1=[1,3,5,4,67,88,54,17]
    for i in l1:
        if i>1:
            for j in range(2,i):
                if (i%j)==0:
                    print("number {} is not a prime number".format(i))
                    break
            else:
                print("number {} is a prime number".format(i))

prime()
